{
  "name": "express-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "cd client && npm install",
    "client": "npm start --prefix client",
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "DEBUG=express-api:* npm run devstart"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.2",
    "express": "^4.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.9"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
